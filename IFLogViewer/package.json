{
  "name": "if-log-viewer",
  "private": true,
  "version": "1.0.0",
  "description": "IF Log Viewer Application",
  "type": "module",
  "scripts": {
    "dbg": "vite --mode dbg",
    "dev": "vite --mode dev",
    "build": "vite build",
    "rebuild": "npm --prefix ../if-web-common-react run rebuild && npm install && rimraf node_modules/.vite && vite build",
    "preview": "vite preview",
    "dev:fresh": "rimraf node_modules/.vite && vite",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@if/styles": "file:../IF.Styles",
    "@if/web-common": "file:../if-web-common",
    "@if/web-common-react": "file:../if-web-common-react",
    "@microsoft/signalr": "^8.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.16"
  }
}
