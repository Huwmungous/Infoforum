    "@microsoft/signalr": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.1.18",
    "vite": "^5.0.8"
  }
}