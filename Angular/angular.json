{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "testapp": {
      "projectType": "application",
      "root": "projects/testapp",
      "sourceRoot": "projects/testapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/testapp",
            "index": "projects/testapp/src/index.html",
            "main": "projects/testapp/src/main.ts",
            "tsConfig": "projects/testapp/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/testapp/src/assets",
                "output": "/assets"
              },
              "projects/testapp/src/favicon.ico"
            ],
            "styles": [
              "projects/testapp/src/styles.scss",
              "node_modules/@angular/material/prebuilt-themes/azure-blue.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "300kb",
                  "maximumError": "350kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "testapp:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "testapp:build"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "testapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/testapp/src/test.ts",
            "tsConfig": "projects/testapp/tsconfig.spec.json",
            "karmaConfig": "projects/testapp/karma.conf.js",
            "assets": [
              "projects/testapp/src/favicon.ico",
              "projects/testapp/src/assets"
            ],
            "styles": [
              "projects/testapp/src/styles.scss",
              "node_modules/@angular/material/prebuilt-themes/azure-blue.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "tsConfig": [
              "projects/testapp/tsconfig.app.json",
              "projects/testapp/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ifauth-lib": {
      "projectType": "library",
      "root": "projects/ifauth-lib",
      "sourceRoot": "projects/ifauth-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ifauth-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ifauth-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ifauth-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ifauth-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "deepseek-gui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/deepseek-gui",
      "sourceRoot": "projects/deepseek-gui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/deepseek-gui",
            "index": "projects/deepseek-gui/src/index.html",
            "main": "projects/deepseek-gui/src/main.ts",
            "tsConfig": "projects/deepseek-gui/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/deepseek-gui/src/assets",
                "output": "/assets"
              },
              "projects/deepseek-gui/src/favicon.ico"
            ],
            "styles": [
              "projects/deepseek-gui/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "@angular/material/prebuilt-themes/cyan-orange.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "baseHref": "/intelligence/",
              "fileReplacements": [
                {
                  "replace": "projects/deepseek-gui/src/environments/environment.ts",
                  "with": "projects/deepseek-gui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "300kb",
                  "maximumError": "350kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "baseHref": "/",
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "deepseek-gui:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "deepseek-gui:build:production"
            },
            "development": {
              "buildTarget": "deepseek-gui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "deepseek-gui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/deepseek-gui/src/test.ts",
            "tsConfig": "projects/deepseek-gui/tsconfig.spec.json",
            "karmaConfig": "projects/deepseek-gui/karma.conf.js",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "assets": [
              "projects/deepseek-gui/src/favicon.ico",
              "projects/deepseek-gui/src/assets"
            ],
            "styles": [
              "projects/deepseek-gui/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "@angular/material/prebuilt-themes/cyan-orange.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "tsConfig": [
              "projects/deepseek-gui/tsconfig.app.json",
              "projects/deepseek-gui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
